  "bugs": {
    "url": "https://github.com/nandha-marimuthu/lazyLoad/issues"
  },
  "homepage": "https://github.com/nandha-marimuthu/lazyLoad#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^5.3.0",
    "dot": "^1.1.3",
    "dotenv": "^8.2.0",
    "env": "0.0.2",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.10.11",
    "nodemon": "^2.0.6",
    "update-node": "^0.1.0",
    "react-icons": "^3.11.0"
  }
